<?xml version="1.0" encoding="UTF-8"?>
<gui platform="android">

	<actions>
		<action method="onCreateContextMenu"
			class="android.view.View.OnCreateContextMenuListener"
			category="INSOURCE"
			type="top-event"/>
		<action method="onContextItemSelected" class="android.app.Activity"
			category="INSOURCE"
			type="top-event"/>

		<action method="onCreateOptionsMenu"  class="android.app.Activity"
			category="INSOURCE"
			type="top-event"/>
		<action method="onPrepareOptionsMenu"  class="android.app.Activity"
			category="INSOURCE"
			type="top-event"/>
		<action method="onOptionsItemSelected"  class="android.app.Activity"
			category="INSOURCE"
			type="top-event"/>

		<action method="onBackPresses"  class="android.app.Activity"
