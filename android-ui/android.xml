		<action-ref id="android.widget.AdapterView.OnItemSelectedListener#onItemSelected"/>
		<action-ref id="android.widget.AdapterView.OnItemSelectedListener#onNothingSelected"/>
	</widget>

	<widget class="android.widget.Spinner">
		<action-ref id="android.widget.AdapterView.OnItemSelectedListener#onItemSelected"/>
		<action-ref id="android.widget.AdapterView.OnItemSelectedListener#onNothingSelected"/>
	</widget>

